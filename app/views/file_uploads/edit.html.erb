<h2>Edit File</h2>

<%= form_with model: @file_upload, local: true do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </div>

  <div>
    <%= form.submit "Update File" %>
  </div>
<% end %>
